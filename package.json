{
  "name": "eslint-plugin-markdown-frontmatter-schema",
  "version": "0.0.1",
  "description": "An ESLint rule that verifies Markdown frontmatter against a JSON schema.",
  "keywords": [
    "lint",
    "yaml",
    "eslint",
    "validation",
    "linting",
    "json-schema",
    "linter",
    "frontmatter",
    "rule",
    "ajv",
    "markdown",
    "vscode",
    "tooling"
  ],
  "license": "ISC",
  "author": "Julian Cataldo <contact@juliancataldo.com>",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "files": [
    "dist",
    "!dist/test"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf ./dist",
    "dev": "tsc --watch",
    "release": "semantic-release",
    "test": "c8 node --enable-source-maps --test dist/tests/**.test.js",
    "test:dev": "node --enable-source-maps --watch --test dist/tests/**.test.js"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^12.0.2",
    "@eslint/markdown": "^6.4.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "eslint": "^9.26.0",
    "synckit": "^0.11.5",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "10.3.3",
    "@semantic-release/npm": "12.0.1",
    "@types/eslint": "^9.6.1",
    "@types/json-schema": "^7.0.15",
    "@types/mdast": "^4.0.4",
    "@types/node": "^22.15.18",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "c8": "^10.1.3",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-perfectionist": "^4.12.3",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "prettier": "^3.5.3",
    "semantic-release": "^24.1.1",
    "typescript": "^5.8.3"
  }
}
